{
  "name": "sssc-app",
  "version": "0.0.1",
  "description": "Sitka Sound Science Center Aquarium App",
  "author": "Kyle Kosma",
  "private": true,
  "main": "main.js",
  "config": {
    "nuxt": {
      "host": "localhost",
      "port": "3000"
    }
  },
  "build": {
    "asar": false,
    "appId": "",
    "directories": {},
    "mac": {
      "identity": null
    },
    "win": {
      "signAndEditExecutable": false
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "dev": "NODE_ENV=NDEV nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "e-dev": "cross-env NODE_ENV=DEV electron .",
    "e-build": "nuxt build && electron-builder -m"
  },
  "dependencies": {
    "leaflet": "^1.3.1",
    "markdown-with-front-matter-loader": "^0.1.0",
    "nuxt": "^1.4.0",
    "mt-files-downloader": "git+https://git@github.com/Fer0x/node-mt-files-downloader.git",
    "vue-loader": "13.7.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.4",
    "electron": "2.0.1",
    "electron-builder": "^20.8.1",
    "electron-devtools-installer": "^2.2.3",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs": "0.0.1-security",
    "require-all": "^2.2.0"
  }
}
